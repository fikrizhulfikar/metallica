<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.phpdocx.com/main" xmlns:pdx="http://www.phpdocx.com/main">
  <xs:element name="document">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:config"/>
        <xs:element ref="pdx:settings"/>
        <xs:element ref="pdx:content"/>
        <xs:element ref="pdx:templateVariables"/>
        <xs:element ref="pdx:utilities"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="config">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:template"/>
        <xs:element ref="pdx:output"/>
        <xs:element ref="pdx:crypto"/>
        <xs:element ref="pdx:sign"/>
        <xs:element ref="pdx:transformDocument"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="template">
    <xs:complexType>
      <xs:attribute name="path" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="output">
    <xs:complexType>
      <xs:attribute name="name" use="required" form="qualified"/>
      <xs:attribute name="type" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="crypto">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:encrypt"/>
        <xs:element ref="pdx:protect"/>
        <xs:element ref="pdx:removeProtection"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="encrypt">
    <xs:complexType>
      <xs:attribute name="password" use="required" form="qualified"/>
      <xs:attribute name="src" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="protect">
    <xs:complexType>
      <xs:attribute name="password" use="required" form="qualified"/>
      <xs:attribute name="src" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
      <xs:attribute name="type" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="removeProtection">
    <xs:complexType>
      <xs:attribute name="src" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="sign">
    <xs:complexType>
      <xs:attribute name="X509Certificate" use="required" form="qualified"/>
      <xs:attribute name="password" use="required" form="qualified"/>
      <xs:attribute name="privateKey" use="required" form="qualified"/>
      <xs:attribute name="src" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="transformDocument">
    <xs:complexType>
      <xs:attribute name="src" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
      <xs:attribute name="toc" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="settings">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:addBackgroundImage"/>
        <xs:element ref="pdx:addLineNumbering"/>
        <xs:element ref="pdx:addMacroFromDoc"/>
        <xs:element ref="pdx:addPageBorders"/>
        <xs:element ref="pdx:addProperties"/>
        <xs:element ref="pdx:createListStyle"/>
        <xs:element ref="pdx:createParagraphStyle"/>
        <xs:element ref="pdx:docxSettings"/>
        <xs:element ref="pdx:importHeadersAndFooters"/>
        <xs:element ref="pdx:importStyles"/>
        <xs:element ref="pdx:modifyPageLayout"/>
        <xs:element ref="pdx:parseStyles"/>
        <xs:element ref="pdx:setBackgroundColor"/>
        <xs:element ref="pdx:setDefaultFont"/>
        <xs:element ref="pdx:setEncodeUTF8"/>
        <xs:element ref="pdx:setLanguage"/>
        <xs:element ref="pdx:setMarkAsFinal"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="addBackgroundImage">
    <xs:complexType>
      <xs:attribute name="src" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="addLineNumbering">
    <xs:complexType>
      <xs:attribute name="countBy" use="required" form="qualified"/>
      <xs:attribute name="distance" use="required" form="qualified"/>
      <xs:attribute name="restart" use="required" form="qualified"/>
      <xs:attribute name="start" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="addMacroFromDoc">
    <xs:complexType>
      <xs:attribute name="src" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="addPageBorders">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:top"/>
        <xs:element ref="pdx:right"/>
        <xs:element ref="pdx:bottom"/>
        <xs:element ref="pdx:left"/>
      </xs:sequence>
      <xs:attribute name="color" use="required" form="qualified"/>
      <xs:attribute name="display" use="required" form="qualified"/>
      <xs:attribute name="offsetFrom" use="required" form="qualified"/>
      <xs:attribute name="space" use="required" form="qualified"/>
      <xs:attribute name="style" use="required" form="qualified"/>
      <xs:attribute name="width" use="required" form="qualified"/>
      <xs:attribute name="zOrder" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="addProperties">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:title"/>
        <xs:element ref="pdx:subject"/>
        <xs:element ref="pdx:creator"/>
        <xs:element ref="pdx:keywords"/>
        <xs:element ref="pdx:description"/>
        <xs:element ref="pdx:category"/>
        <xs:element ref="pdx:manager"/>
        <xs:element ref="pdx:company"/>
        <xs:element ref="pdx:contentStatus"/>
        <xs:element ref="pdx:customProperties"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="subject" type="xs:NCName"/>
  <xs:element name="creator" type="xs:NCName"/>
  <xs:element name="keywords" type="xs:NCName"/>
  <xs:element name="description" type="xs:NCName"/>
  <xs:element name="category" type="xs:NCName"/>
  <xs:element name="manager" type="xs:NCName"/>
  <xs:element name="company" type="xs:NCName"/>
  <xs:element name="contentStatus" type="xs:NCName"/>
  <xs:element name="customProperties">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:custom"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="custom">
    <xs:complexType mixed="true">
      <xs:attribute name="name" use="required" form="qualified"/>
      <xs:attribute name="type" use="required" form="qualified" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="createListStyle">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:item"/>
      </xs:sequence>
      <xs:attribute name="name" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="createParagraphStyle">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:paragraphStyle"/>
      </xs:sequence>
      <xs:attribute name="name" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="docxSettings">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:autoHyphenation"/>
        <xs:element ref="pdx:bookFoldPrinting"/>
        <xs:element ref="pdx:bookFoldPrintingSheets"/>
        <xs:element ref="pdx:bookFoldRevPrinting"/>
        <xs:element ref="pdx:bordersDoNotSurroundHeader"/>
        <xs:element ref="pdx:bordersDoNotSurroundFoote"/>
        <xs:element ref="pdx:consecutiveHyphenLimit"/>
        <xs:element ref="pdx:defaultTableStyle"/>
        <xs:element ref="pdx:defaultTabStop"/>
        <xs:element ref="pdx:documentType"/>
        <xs:element ref="pdx:doNotHyphenateCaps"/>
        <xs:element ref="pdx:doNotShadeFormData"/>
        <xs:element ref="pdx:gutterAtTop"/>
        <xs:element ref="pdx:hideGrammaticalErrors"/>
        <xs:element ref="pdx:hideSpellingErrors"/>
        <xs:element ref="pdx:hyphenationZone"/>
        <xs:element ref="pdx:mirrorMargins"/>
        <xs:element ref="pdx:noPunctuationKerning"/>
        <xs:element ref="pdx:printTwoOnOne"/>
        <xs:element ref="pdx:savePreviewPicture"/>
        <xs:element ref="pdx:trackRevisions"/>
        <xs:element ref="pdx:updateFields"/>
        <xs:element ref="pdx:view"/>
        <xs:element ref="pdx:zoom"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="autoHyphenation">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="bookFoldPrinting">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="bookFoldPrintingSheets">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="bookFoldRevPrinting">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="bordersDoNotSurroundHeader">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="bordersDoNotSurroundFoote">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="consecutiveHyphenLimit">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="defaultTableStyle">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="defaultTabStop">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="documentType">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="doNotHyphenateCaps">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="doNotShadeFormData">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="gutterAtTop">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="hideGrammaticalErrors">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="hideSpellingErrors">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="hyphenationZone">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="mirrorMargins">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="noPunctuationKerning">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="printTwoOnOne">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="savePreviewPicture">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="trackRevisions">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="updateFields">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="view">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="zoom">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="importHeadersAndFooters">
    <xs:complexType>
      <xs:attribute name="src" use="required" form="qualified"/>
      <xs:attribute name="type" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="importStyles">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:styles"/>
      </xs:sequence>
      <xs:attribute name="src" use="required" form="qualified"/>
      <xs:attribute name="styleIdentifier" use="required" form="qualified"/>
      <xs:attribute name="type" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="styles">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:style"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="style">
    <xs:complexType>
      <xs:attribute name="name" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="modifyPageLayout">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="pdx:layout">
          <xs:attribute name="paperType" use="required" form="qualified"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="parseStyles">
    <xs:complexType>
      <xs:attribute name="path" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="setBackgroundColor">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="setDefaultFont">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="setEncodeUTF8">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="setLanguage">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="setMarkAsFinal">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="templateVariables">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:clearBlocks"/>
        <xs:element ref="pdx:deleteTemplateBlocks"/>
        <xs:element ref="pdx:modifyInputFields"/>
        <xs:element ref="pdx:processTemplate"/>
        <xs:element ref="pdx:removeTemplateVariable"/>
        <xs:element ref="pdx:replaceListVariable"/>
        <xs:element ref="pdx:replaceTableVariable"/>
        <xs:element ref="pdx:replacePlaceholderImage"/>
        <xs:element ref="pdx:replaceVariableByHTML"/>
        <xs:element ref="pdx:replaceVariableByText"/>
        <xs:element ref="pdx:replaceVariableByExternalFile"/>
        <xs:element ref="pdx:replaceVariableByWordFragment"/>
        <xs:element ref="pdx:replaceVariableByWordML"/>
        <xs:element ref="pdx:setTemplateSymbol"/>
      </xs:sequence>
      <xs:attribute name="preprocessedTemplate" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="clearBlocks">
    <xs:complexType/>
  </xs:element>
  <xs:element name="deleteTemplateBlocks">
    <xs:complexType>
      <xs:attribute name="variableName" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="modifyInputFields">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:inputField"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="inputField">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:data"/>
      </xs:sequence>
      <xs:attribute name="variableName" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="processTemplate">
    <xs:complexType/>
  </xs:element>
  <xs:element name="removeTemplateVariable">
    <xs:complexType>
      <xs:attribute name="removeType" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
      <xs:attribute name="variableName" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="replaceListVariable">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:data"/>
      </xs:sequence>
      <xs:attribute name="firstMatch" use="required" form="qualified"/>
      <xs:attribute name="parseLineBreaks" use="required" form="qualified"/>
      <xs:attribute name="variableName" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="replaceTableVariable">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:data"/>
      </xs:sequence>
      <xs:attribute name="firstMatch" use="required" form="qualified"/>
      <xs:attribute name="parseLineBreaks" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="replacePlaceholderImage">
    <xs:complexType>
      <xs:attribute name="dpi" use="required" form="qualified"/>
      <xs:attribute name="firstMatch" use="required" form="qualified"/>
      <xs:attribute name="height" use="required" form="qualified"/>
      <xs:attribute name="name" use="required" form="qualified"/>
      <xs:attribute name="path" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
      <xs:attribute name="width" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="replaceVariableByHTML">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:data"/>
        <xs:element ref="pdx:wordStyles"/>
      </xs:sequence>
      <xs:attribute name="baseURL" use="required" form="qualified"/>
      <xs:attribute name="customListStyles" use="required" form="qualified"/>
      <xs:attribute name="downloadImages" use="required" form="qualified"/>
      <xs:attribute name="filter" use="required" form="qualified"/>
      <xs:attribute name="isFile" use="required" form="qualified"/>
      <xs:attribute name="parseAnchors" use="required" form="qualified"/>
      <xs:attribute name="parseDivsAsPs" use="required" form="qualified"/>
      <xs:attribute name="parseFloats" use="required" form="qualified"/>
      <xs:attribute name="replaceType" use="required" form="qualified"/>
      <xs:attribute name="variableName" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="replaceVariableByText">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:variable"/>
      </xs:sequence>
      <xs:attribute name="firstMatch" use="required" form="qualified"/>
      <xs:attribute name="parseLineBreaks" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="replaceVariableByExternalFile">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:variable"/>
      </xs:sequence>
      <xs:attribute name="firstMatch" use="required" form="qualified"/>
      <xs:attribute name="matchSource" use="required" form="qualified"/>
      <xs:attribute name="preprocess" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="replaceVariableByWordFragment">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:variable"/>
      </xs:sequence>
      <xs:attribute name="firstMatch" use="required" form="qualified"/>
      <xs:attribute name="parseLineBreaks" use="required" form="qualified"/>
      <xs:attribute name="replaceType" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="replaceVariableByWordML">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:variable"/>
      </xs:sequence>
      <xs:attribute name="firstMatch" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
      <xs:attribute name="type" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="setTemplateSymbol">
    <xs:complexType>
      <xs:attribute name="symbolValue" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="utilities">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pdx:mergeDocx"/>
        <xs:element ref="pdx:mergePdf"/>
        <xs:element ref="pdx:parseCheckboxes"/>
        <xs:element ref="pdx:removeChapter"/>
        <xs:element ref="pdx:replaceChartData"/>
        <xs:element ref="pdx:searchAndHighlight"/>
        <xs:element ref="pdx:searchAndRemove"/>
        <xs:element ref="pdx:searchAndReplace"/>
        <xs:element ref="pdx:setLineNumbering"/>
        <xs:element ref="pdx:watermarkDocx"/>
        <xs:element ref="pdx:watermarkRemove"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="mergeDocx">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:docx"/>
      </xs:sequence>
      <xs:attribute name="forceLatestStyles" use="required" form="qualified"/>
      <xs:attribute name="lineBreaks" use="required" form="qualified"/>
      <xs:attribute name="mergeType" use="required" form="qualified"/>
      <xs:attribute name="numbering" use="required" form="qualified"/>
      <xs:attribute name="preserveStyleDefaults" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="docx">
    <xs:complexType>
      <xs:attribute name="path" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="mergePdf">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:pdf"/>
      </xs:sequence>
      <xs:attribute name="target" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="pdf">
    <xs:complexType>
      <xs:attribute name="path" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="parseCheckboxes">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="pdx:checkbox"/>
      </xs:sequence>
      <xs:attribute name="src" use="required" form="qualified"/>
      <xs:attribute name="target" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="checkbox">
    <xs:complexType>
      <xs:attribute name="value" use="required" form="qualified"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="removeChapter">
    <xs:complexType>
      <xs:attribute name="searchTerm" use="required" form="qualified"/>
      <xs:attribute name="src" use="required" form="qualified"/>
